{
  "name": "semirara",
  "version": "0.0.1",
  "description": "wiki",
  "main": "server/run.js",
  "engines": {
    "node": "5.x",
    "npm": "3.x"
  },
  "scripts": {
    "start": "node server/run.js",
    "test": "npm run eslint",
    "mocha": "mocha */test/*.es6",
    "eslint": "eslint */*.es6 */*/*.es6 */*/*/*.es6",
    "build": "babel server/src/ --out-dir server/dist/ --source-maps inline",
    "watch": "parallelshell 'chokidar \"*/**.es6\" -c \"npm run eslint\"' 'npm run build -- --watch'",
    "clean": "rm -rf server/dist"
  },
  "keywords": [
    "wiki",
    "outline"
  ],
  "author": "Sho Hashimoto <hashimoto@shokai.org>",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "debug": "^2.2.0",
    "koa": "^2.0.0-alpha.3",
    "koa-66": "^0.8.4",
    "koa-logger": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "eslint": "^2.2.0",
    "eslint-plugin-if-in-test": "^0.2.0",
    "mocha": "^2.4.5",
    "parallelshell": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shokai/semirara.git"
  },
  "bugs": {
    "url": "https://github.com/shokai/semirara/issues"
  },
  "homepage": "https://github.com/shokai/semirara#readme"
}
