{
  "private": true,
  "name": "semirara",
  "version": "0.0.1",
  "description": "wiki",
  "main": "run-server.js",
  "engines": {
    "node": "5.x",
    "npm": "3.x"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "node run-server.js",
    "start:dev": "DEBUG=semirara*,koa* node-dev run-server.js",
    "test": "npm run eslint && npm run mocha",
    "mocha": "mocha src/*/test/*.es6 --compilers js:babel-register",
    "eslint": "eslint src/*/{*,**/*}.{js,es6,jsx}",
    "browserify": "browserify --verbose --extension=.jsx --extension=.es6 -t [ babelify ] -g uglifyify src/client/index.es6 -o public/dist/index.js",
    "watchify": "watchify --verbose --debug --extension=.jsx --extension=.es6 -t [ babelify ] src/client/index.es6 -o public/dist/index.js",
    "build": "npm run babel && npm run browserify && npm run stylus",
    "stylus": "stylus -I node_modules/nib/lib -I node_modules/highlight.js/styles --include-css src/client/styl/index.styl -o public/dist/index.css",
    "babel": "babel src/ --out-dir dist/ --source-maps inline",
    "watch": "parallelshell 'chokidar \"*/**.{es6,jsx}\" -c \"npm run test\"' 'npm run babel -- --watch' 'npm run watchify' 'npm run stylus -- --watch'",
    "clean": "rm -rf dist/* public/dist/*"
  },
  "keywords": [
    "wiki",
    "outline"
  ],
  "author": "Sho Hashimoto <hashimoto@shokai.org>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.9.1",
    "babel-cli": "^6.5.1",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-regenerator-runtime": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "classnames": "^2.2.3",
    "clone": "^1.0.2",
    "cookie": "^0.2.3",
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "highlight.js": "^9.3.0",
    "koa": "^2.0.0-alpha.3",
    "koa-66": "^0.8.4",
    "koa-convert": "^1.2.0",
    "koa-logger": "^2.0.0",
    "koa-react-view": "^2.0.0",
    "koa-static": "^2.0.0",
    "lodash.uniq": "^4.2.1",
    "md5": "^2.0.0",
    "memjs": "^0.9.0",
    "mongoose": "^4.4.5",
    "mongoose-auto-increment": "^5.0.1",
    "nib": "^1.1.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "reactify": "^1.1.1",
    "redux": "^3.4.0",
    "shortid": "^2.2.6",
    "socket.io": "^1.4.5",
    "socket.io-client": "^1.4.5",
    "socket.io-request": "^0.0.4",
    "stylus": "^0.54.2",
    "uglifyify": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "eslint": "^2.7.0",
    "eslint-plugin-if-in-test": "^0.2.0",
    "eslint-plugin-react": "^4.3.0",
    "mocha": "^2.4.5",
    "node-dev": "^3.1.0",
    "parallelshell": "^2.0.0",
    "watchify": "^3.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shokai/semirara.git"
  },
  "bugs": {
    "url": "https://github.com/shokai/semirara/issues"
  },
  "homepage": "https://github.com/shokai/semirara#readme"
}
